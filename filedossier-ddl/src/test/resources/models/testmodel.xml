<?xml version="1.0" encoding="UTF-8"?>
<package
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="urn:ru:ilb:filedossier:ddl"
    xsi:schemaLocation="urn:ru:ilb:filedossier:ddl ../../../../../filedossier-ddl/src/main/resources/schemas/filedossier/ddl.xsd">
    <code>testmodel</code>
    <name>Тестовый пакет досье</name>
    <dossier>
        <code>TEST</code>
        <name>Тестовое досье</name>
        <dossierFile>
            <code>file1</code>
            <name>Файл 1</name>
            <readonly>false</readonly>
            <required>true</required>
            <hidden>false</hidden>
            <mediaType>application/xml</mediaType>
        </dossierFile>
        <dossierFile>
            <code>fairpricecalc</code>
            <name>Тест имя</name>
            <readonly>false</readonly>
            <required>true</required>
            <hidden>false</hidden>
            <mediaType>application/xml</mediaType>
            <representation>
                <mediaType>application/vnd.oasis.opendocument.spreadsheet</mediaType>
                <stylesheet>representations/fairpriceorder/content.xsl</stylesheet>
                <template>representations/fairpriceorder/template.ods</template>
                <schema></schema>
            </representation>
        </dossierFile>
<dossierFile>
            <code>jurnals</code>
            <name>${name}</name>
            <readonly>false</readonly>
            <required>true</required>
            <hidden>false</hidden>
            <mediaType>application/pdf</mediaType>
            <representation>
                <mediaType>application/pdf</mediaType>
                <template>representations/jurnals/example.xml</template>
                <schema>https://${ru.bystrobank.apps.meta.url}/stylesheets/doctemplates/jurnals/percentsheet.xsl&amp;xsd=https://${ru.bystrobank.apps.meta.url}/schemas/doctemplates/jurnals/percentsheet.xsd</schema>
            </representation>
        </dossierFile>
    </dossier>
</package>
